    _name = Strings2.upper(_name);
    uint256 _searchFor = _reverseSearch[_name];
    FantomsArtName memory x = _tokenIdToStruct[_searchFor];
    return x._owner;
  }

  function invertBool(bool _bool) private pure returns(bool) {
    if (_bool) {
      return false;
    } else {
      return true;

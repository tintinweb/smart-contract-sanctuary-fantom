pragma solidity ^0.8.0;
abstract contract StrategyLPBase is StrategyBase {
    using SafeERC20 for IERC20;
    using SafeERC20 for IUniswapV2Pair;

    IERC20 public lpAsset0;
    IERC20 public lpAsset1;

    constructor(
        IERC20            _stakedToken,
        IERC20[] memory   _rewardTokens,
        IERC20            _feeToken,
        TokenSwapper      _tokenSwapper,
        CrackVault        _vault,
        uint256           _withdrawalFeePercentTimes10
    )
        StrategyBase(_stakedToken, _rewardTokens, _feeToken, _tokenSwapper, _vault, _withdrawalFeePercentTimes10)
    {
        lpAsset0 = IERC20(IUniswapV2Pair(address(_stakedToken)).token0());
        lpAsset1 = IERC20(IUniswapV2Pair(address(_stakedToken)).token1());
    }

    // Adds liquidity to AMM and gets more LP tokens.
    function convertRewardsToStakedToken()
        internal
        override(StrategyBase)
        returns (uint256 stakedTokenAmount)
    {
        for (uint8 i = 0; i < rewardTokens.length; i++) {
            IERC20 rewardToken = rewardTokens[i];
            uint256 rewardTokenTotal = rewardToken.balanceOf(address(this));
            uint256 rewardTokenHalf  = rewardTokenTotal / 2;
            if (rewardTokenHalf > 0) {
                if (lpAsset0 != rewardToken) {
                    rewardToken.safeApprove(address(tokenSwapper), rewardTokenHalf);
                    tokenSwapper.swap(rewardToken, rewardTokenHalf, lpAsset0, address(this));
                    rewardToken.safeApprove(address(tokenSwapper), 0);
                }

                if (lpAsset1 != rewardToken) {
                    rewardToken.safeApprove(address(tokenSwapper), rewardTokenHalf);
                    tokenSwapper.swap(rewardToken, rewardTokenHalf, lpAsset1, address(this));
                    rewardToken.safeApprove(address(tokenSwapper), 0);
                }
            }
        }
        uint256 asset0Bal = lpAsset0.balanceOf(address(this));
        uint256 asset1Bal = lpAsset1.balanceOf(address(this));
        if ((asset0Bal > 0) && (asset1Bal > 0)) {
            IUniswapRouterETH unirouter = tokenSwapper.getRouter(lpAsset0, lpAsset1);
            lpAsset0.safeApprove(address(unirouter), asset0Bal);
            lpAsset1.safeApprove(address(unirouter), asset1Bal);
            ( , , uint256 amountLP) = unirouter.addLiquidity(address(lpAsset0), address(lpAsset1), asset0Bal, asset1Bal, 0, 0, address(this), type(uint256).max);
            lpAsset0.safeApprove(address(unirouter), 0);
            lpAsset1.safeApprove(address(unirouter), 0);
            stakedTokenAmount = amountLP;
        }
        return stakedTokenAmount;
    }

    function resetValues(
        IERC20            _stakedToken,
        IERC20[] calldata _rewardTokens,
        IERC20            _feeToken,
        TokenSwapper      _tokenSwapper,
        uint256           _withdrawalFeePercentTimes10
    ) internal override(StrategyBase) {
        StrategyBase.resetValues(_stakedToken, _rewardTokens, _feeToken, _tokenSwapper, _withdrawalFeePercentTimes10);
        lpAsset0 = IERC20(IUniswapV2Pair(address(_stakedToken)).token0());
        lpAsset1 = IERC20(IUniswapV2Pair(address(_stakedToken)).token1());
    }
}



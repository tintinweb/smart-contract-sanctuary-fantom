// File contracts/common/FeeManager.sol

pragma solidity ^0.8.13;
// Mixin contract that receives one or more "reward" tokens, converts them to the
// specified `feeToken`, and distributes them to any of the registered fee
// recipients. Fee recipients can be registered with a callback if they need to
// be notified when they receive fees.
contract FeeManager {
    using SafeERC20 for IERC20;

    struct FeeRecipient {
        bool    exists;
        string  tag;
        address addr;
        uint256 feePercentTimes100;
        bytes4  callbackSelector;
    }

    uint256 constant public MAXFEE = 10000;

    TokenSwapper public tokenSwapper;
    IERC20       public feeToken;

    mapping(address => FeeRecipient) public feeRecipients;
    address[]                        public feeRecipientAddresses;
    uint256                          public msgSenderFeePercentTimes100;
    uint256                          public totalFeePercentTimes100;

    constructor(IERC20 _feeToken, TokenSwapper _tokenSwapper) {
        feeToken     = _feeToken;
        tokenSwapper = _tokenSwapper;
    }

    function getFeeRecipient(address addr) public view returns (FeeRecipient memory) {
        return feeRecipients[addr];
    }

    function allFeeRecipientAddresses() public view returns (address[] memory) {
        return feeRecipientAddresses;
    }

    error FeesTooHigh(uint256 proposed);

    function _upsertFeeRecipient(address addr, string calldata tag, uint256 feePercentTimes100, bytes4 callbackSelector) internal virtual {
        if (!feeRecipients[addr].exists) {
            feeRecipientAddresses.push(addr);
            totalFeePercentTimes100 += feePercentTimes100;
        } else {
            totalFeePercentTimes100 = (totalFeePercentTimes100 - feeRecipients[addr].feePercentTimes100) + feePercentTimes100;
        }
        if (totalFeePercentTimes100 >= MAXFEE) revert FeesTooHigh(totalFeePercentTimes100);
        feeRecipients[addr] = FeeRecipient({
            exists: true,
            tag: tag,
            addr: addr,
            feePercentTimes100: feePercentTimes100,
            callbackSelector: callbackSelector
        });
    }

    function _removeFeeRecipient(address addr) internal virtual {
        for (uint8 i = 0; i < feeRecipientAddresses.length; i++) {
            if (feeRecipientAddresses[i] == addr) {
                if (i == feeRecipientAddresses.length - 1) {
                    feeRecipientAddresses.pop();
                } else {
                    feeRecipientAddresses[i] = feeRecipientAddresses[feeRecipientAddresses.length - 1];
                    feeRecipientAddresses.pop();
                }
                break;
            }
        }
        totalFeePercentTimes100 -= feeRecipients[addr].feePercentTimes100;
        delete feeRecipients[addr];
    }

    function _setContracts(IERC20 _feeToken, TokenSwapper _tokenSwapper) internal virtual {
        feeToken     = _feeToken;
        tokenSwapper = _tokenSwapper;
    }

    function _setMsgSenderFeePercentTimes100(uint256 _msgSenderFeePercentTimes100) internal virtual {
        totalFeePercentTimes100 = (totalFeePercentTimes100 - msgSenderFeePercentTimes100) + _msgSenderFeePercentTimes100;
        if (totalFeePercentTimes100 >= MAXFEE) revert FeesTooHigh(totalFeePercentTimes100);

        msgSenderFeePercentTimes100 = _msgSenderFeePercentTimes100;
    }

    function _calculateFees(TokenAndAmount[] memory inputTokens)
        internal
        virtual
        view
        returns (TokenAndAmount[] memory fees, TokenAndAmount[] memory remaining)
    {
        fees      = new TokenAndAmount[](inputTokens.length);
        remaining = new TokenAndAmount[](inputTokens.length);

        for (uint8 i = 0; i < inputTokens.length; i++) {
            uint256 feeAmount = (inputTokens[i].amount * totalFeePercentTimes100) / MAXFEE;

            fees[i] = TokenAndAmount({
                token:  inputTokens[i].token,
                amount: feeAmount
            });

            remaining[i] = TokenAndAmount({
                token:  inputTokens[i].token,
                amount: inputTokens[i].amount - feeAmount
            });
        }
        return (fees, remaining);
    }

    function _convertToFeeTokensAndDistribute(IERC20[] memory tokensToConvert, bool disregardMsgSender)
        internal
        virtual
        returns (uint256 totalFeeTokens, TokenAndAmount[] memory remaining)
    {
        TokenAndAmount[] memory tokensAndAmounts = new TokenAndAmount[](tokensToConvert.length);

        for (uint8 i = 0; i < tokensToConvert.length; i++) {
            tokensAndAmounts[i] = TokenAndAmount({
                token: tokensToConvert[i],
                amount: tokensToConvert[i].balanceOf(address(this))
            });
        }
        return _convertToFeeTokensAndDistribute(tokensAndAmounts, disregardMsgSender);
    }

    function _convertToFeeTokensAndDistribute(TokenAndAmount[] memory tokensToConvert, bool disregardMsgSender)
        internal
        virtual
        returns (uint256 totalFeeTokens, TokenAndAmount[] memory remaining)
    {
        uint256 _totalFeePercentTimes100 = disregardMsgSender ? totalFeePercentTimes100 - msgSenderFeePercentTimes100 : totalFeePercentTimes100;

        TokenAndAmount[] memory feeAmounts;
        (feeAmounts, remaining) = _calculateFees(tokensToConvert);

        for (uint8 i = 0; i < feeAmounts.length; i++) {
            if (tokensToConvert[i].token == feeToken) {
                continue;
            }
            feeAmounts[i].token.safeApprove(address(tokenSwapper), 0);
            feeAmounts[i].token.safeApprove(address(tokenSwapper), feeAmounts[i].amount);
        }

        totalFeeTokens = tokenSwapper.swapAll(feeAmounts, feeToken, address(this));
        if (totalFeeTokens > 0) {
            for (uint8 i = 0; i < feeRecipientAddresses.length; i++) {
                FeeRecipient storage recipient = feeRecipients[feeRecipientAddresses[i]];
                uint256 amount = (totalFeeTokens * recipient.feePercentTimes100) / _totalFeePercentTimes100;
                if (amount > 0) {
                    feeToken.safeTransfer(recipient.addr, amount);
                    if (recipient.callbackSelector != bytes4(0)) {
                        address(recipient.addr).call(abi.encodeWithSelector(recipient.callbackSelector, amount));
                    }
                }
            }

            if (!disregardMsgSender && msgSenderFeePercentTimes100 > 0) {
                feeToken.safeTransfer(msg.sender, (totalFeeTokens * msgSenderFeePercentTimes100) / _totalFeePercentTimes100);
            }
        }
        return (totalFeeTokens, remaining);
    }
}


// File contracts/StrategyBase.sol

pragma solidity ^0.8.13;
//import "./CrackVault.sol";
abstract contract StrategyBase is MultiOwnable, Pausable, FeeManager {
    using SafeERC20 for IERC20;
    using Address for address;

    uint16 public constant version = 2;

    IERC20[]          public rewardTokens;
    IERC20            public stakedToken;
    CrackVault        public vault;
    uint256           public withdrawalFeePercentTimes100;
    uint256           public lastHarvest;

    // Event that is fired each time someone harvests the strat.
    event StratHarvest(address indexed harvester);

    constructor(
        IERC20            _stakedToken,
        IERC20[] memory   _rewardTokens,
        IERC20            _feeToken,
        TokenSwapper      _tokenSwapper,
        CrackVault        _vault,
        uint256           _withdrawalFeePercentTimes100
    )
        MultiOwnable()
        FeeManager(_feeToken, _tokenSwapper)
    {
        stakedToken                  = _stakedToken;
        rewardTokens                 = _rewardTokens;
        vault                        = _vault;
        withdrawalFeePercentTimes100 = _withdrawalFeePercentTimes100;
    }

    // Virtual functions should be overwritten by derived contracts.
    function farmAddress() public view virtual returns (address);
    function farmDeposit(uint256 amount) internal virtual;
    function farmWithdraw(uint256 amount) internal virtual;
    function farmEmergencyWithdraw() internal virtual;
    function farmWithdrawRewards() internal virtual;
    function farmBalanceOfStaked() internal view virtual returns (uint256 balanceOfStaked);
    function convertRewardsToStakedToken() internal virtual returns (uint256 stakedTokenAmount);

    function allRewardTokens() public view returns (IERC20[] memory) {
        return rewardTokens;
    }

    // Moves any stakedTokens held by the strategy into the farm
    function depositStakedTokenIntoFarm() public whenNotPaused returns (uint256 amount) {
        amount = stakedToken.balanceOf(address(this));
        if (amount > 0) {
            stakedToken.safeApprove(farmAddress(), amount);
            farmDeposit(amount);
            stakedToken.safeApprove(farmAddress(), 0);
        }
        return amount;
    }

    // Compounds earnings, updates user balances, and charges performance fee
    function compoundRewards()
        public
        whenNotPaused
        returns (uint256 stakedTokensReinvested, uint256 feeTokensDistributed)
    {
        require(!msg.sender.isContract(), "no contracts");

        farmWithdrawRewards();
        (feeTokensDistributed, ) = FeeManager._convertToFeeTokensAndDistribute(rewardTokens, false);
        stakedTokensReinvested = convertRewardsToStakedToken();
        depositStakedTokenIntoFarm();

        lastHarvest = block.timestamp;
        emit StratHarvest(msg.sender);

        return (stakedTokensReinvested, feeTokensDistributed);
    }

    function compoundRewardsForVaultAccounting()
        public
        whenNotPaused
    {
        require(msg.sender == address(vault), "!vault");

        farmWithdrawRewards();
        FeeManager._convertToFeeTokensAndDistribute(rewardTokens, true);
        convertRewardsToStakedToken();
        depositStakedTokenIntoFarm();

        lastHarvest = block.timestamp;
        emit StratHarvest(msg.sender);
    }

    function withdraw(uint256 _amount) public {
        require(msg.sender == address(vault), "!vault");

        uint256 stakedBal = stakedToken.balanceOf(address(this));

        if (stakedBal < _amount) {
            farmWithdraw(_amount - stakedBal);
            stakedBal = stakedToken.balanceOf(address(this));
            if (stakedBal < _amount) {
                _amount = stakedBal;
            }
        }

        uint256 withdrawalFeeAmount = (_amount * withdrawalFeePercentTimes100) / FeeManager.MAXFEE;
        stakedToken.safeTransfer(address(vault), _amount - withdrawalFeeAmount);
    }

    function totalBalanceOfStaked() public view returns (uint256) {
        return stakedToken.balanceOf(address(this)) + farmBalanceOfStaked();
    }

    /**
     * @dev Called as part of strat migration. Sends all the available funds back to the vault. The
     * strategy can never be used again.
     */
    function retireStrat() public {
        require(msg.sender == address(vault), "!vault");

        compoundRewardsForVaultAccounting();

        farmEmergencyWithdraw();
        stakedToken.safeTransfer(address(vault), stakedToken.balanceOf(address(this)));
        vault = CrackVault(address(0));
    }

    //
    // Admin functions
    //

    /**
     * @dev Rescues random funds stuck that the strat can't handle.
     * @param token address of the token to rescue.
     */
    function inCaseTokensGetStuck(IERC20 token) public onlyOwners {
        token.safeTransfer(msg.sender, token.balanceOf(address(this)));
    }

    // pauses deposits and withdraws all funds from third party systems.
    function panic() public onlyOwners {
        pause();
        farmEmergencyWithdraw();
    }

    function pause() public onlyOwners {
        _pause();
    }

    function unpause() public onlyOwners {
        _unpause();
        depositStakedTokenIntoFarm();
    }

    function resetValues(
        IERC20            _stakedToken,
        IERC20[] calldata _rewardTokens,
        IERC20            _feeToken,
        TokenSwapper      _tokenSwapper,
        uint256           _withdrawalFeePercentTimes100
    ) internal virtual {
        rewardTokens                 = _rewardTokens;
        stakedToken                  = _stakedToken;
        withdrawalFeePercentTimes100 = _withdrawalFeePercentTimes100;

        FeeManager._setContracts(_feeToken, _tokenSwapper);
    }

    function setFeeContracts(IERC20 _feeToken, TokenSwapper _tokenSwapper) public onlyOwners {
        FeeManager._setContracts(_feeToken, _tokenSwapper);
    }

    function upsertFeeRecipient(
        address addr,
        string calldata tag,
        uint256 feePercentTimes100,
        bytes4 callbackSelector
    ) public {
        require(msg.sender == address(vault) || isOwner(msg.sender));
        FeeManager._upsertFeeRecipient(addr, tag, feePercentTimes100, callbackSelector);
    }

    function removeFeeRecipient(address addr) public onlyOwners {
        FeeManager._removeFeeRecipient(addr);
    }

    function setMsgSenderFeePercentTimes100(uint256 _msgSenderFeePercentTimes100) public onlyOwners {
         FeeManager._setMsgSenderFeePercentTimes100(_msgSenderFeePercentTimes100);
    }

    function setWithdrawalFeePercentTimes100(uint256 _withdrawalFeePercentTimes100) public onlyOwners {
        withdrawalFeePercentTimes100 = _withdrawalFeePercentTimes100;
    }
}



        return _withdraw(_owner, _destination, _shares);
    }

    function _withdraw(address _owner, address _destination, uint256 _shares)
        internal
        returns (uint256 _withdrawnLP, uint256 _withdrawnProfit)
    {
        strategy.compoundRewardsForVaultAccounting();

        _withdrawnProfit = profitOwedTo(_owner);
        claimProfit();
        uint256 newUserSharesBalance = balanceOf(_owner) - _shares;
        users[_owner].deficit = int256(newUserSharesBalance * notionalTom)/1e18 - int256(profitOwedTo(_owner));
                                  //                ^                                             ^
                                  // cancel out the entire rectangle                   add back in what's owed
                                  //

        uint256 tokensToWithdraw = sharesToStakedTokens(_shares);
        _burn(_owner, _shares);

        // We first check if we have enough `stakedToken` in the Vault to accommodate
        // the withdrawal request. If not, we take the rest from the Strategy. The
        // case in which this provides gas savings is when `feeToken` == `stakedToken`.
        uint256 tokensInVault = stakedToken().balanceOf(address(this));
        if (tokensInVault < tokensToWithdraw) {
            uint256 toWithdrawFromStrategy = tokensToWithdraw - tokensInVault;
            strategy.withdraw(toWithdrawFromStrategy);

            uint256 diff = stakedToken().balanceOf(address(this)) - tokensInVault;
            if (diff < toWithdrawFromStrategy) {
                tokensToWithdraw = tokensInVault + diff;
            }
        }

        stakedToken().safeTransfer(_destination, tokensToWithdraw);

        return (tokensToWithdraw, _withdrawnProfit);
    }

    //
    // Getter functions
    //

    function stakedToken() public view returns (IERC20) {
        return IERC20(strategy.stakedToken());
    }

    function feeToken() public view returns (IERC20) {
        return IERC20(strategy.feeToken());
    }

    /**
     * @dev It calculates the total underlying value of {token} held by the system.
     * It takes into account the vault contract balance, the strategy contract balance
     *  and the balance deployed in other contracts as part of the strategy.
     */
    function totalBalanceOfStaked() public view returns (uint) {
        return (stakedToken().balanceOf(address(this))) + strategy.totalBalanceOfStaked();
    }

    function sharesToStakedTokens(uint256 shares) public view returns (uint256) {
        return (shares * totalBalanceOfStaked()) / totalSupply();
    }

    function stakedTokensToShares(uint256 tokens) public view returns (uint256) {
        return (tokens * totalSupply()) / totalBalanceOfStaked();
    }

    /**
     * @dev Function for various UIs to display the current value of one of our yield tokens.
     * Returns an uint256 with 18 decimals of how much underlying asset one vault share represents.
     */
    function getPricePerFullShare() public view returns (uint256) {
        return totalSupply() == 0 ? 1e18 : ((totalBalanceOfStaked() * 1e18) / totalSupply());
    }

    //
    // Admin functions
    //

    function migrateToNewStrategy(StrategyBase _strategy) external onlyOwners {
        if (address(strategy) != address(0)) {
            strategy.retireStrat();
        }
        strategy = _strategy;
        depositStakedTokenIntoFarm();
    }

    /**
     * @dev Rescues random funds stuck that the strat can't handle.
     * @param _token address of the token to rescue.
     */
    function inCaseTokensGetStuck(address _token) external onlyOwners {
        // require(_token != address(stakedToken()), "!token");

        uint256 amount = IERC20(_token).balanceOf(address(this));
        IERC20(_token).safeTransfer(msg.sender, amount);
    }
}



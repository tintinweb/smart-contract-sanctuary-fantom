        IERC20            _feeToken,
        TokenSwapper      _tokenSwapper,
        CrackVault        _vault,
        uint256           _withdrawalFeePercentTimes10
    )
        StrategyLPBase(_stakedToken, _rewardTokens, _feeToken, _tokenSwapper, _vault, _withdrawalFeePercentTimes10)
    {
        poolID = _poolID;
        farm   = _farm;
    }

    function farmAddress() public view override(StrategyBase) returns (address) {
        return address(farm);
    }

    function farmWithdraw(uint256 _amount) internal override(StrategyBase) {
        farm.withdraw(poolID, _amount);
    }

    function farmBalanceOfStaked() internal view override(StrategyBase) returns (uint256) {
        (uint256 _amount, ) = farm.userInfo(poolID, address(this));
        return _amount;
    }

    function farmWithdrawRewards() internal override(StrategyBase) {
        farm.deposit(poolID, 0);
    }

    function farmEmergencyWithdraw() internal override(StrategyBase) {
        farm.emergencyWithdraw(poolID);
    }

    function farmDeposit(uint256 amount) internal whenNotPaused override(StrategyBase) {
        farm.deposit(poolID, amount);
    }


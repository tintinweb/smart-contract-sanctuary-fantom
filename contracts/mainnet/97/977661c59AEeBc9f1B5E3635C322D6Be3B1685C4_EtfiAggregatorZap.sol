pragma solidity ^0.8.9;

import "./EtfiIndex.sol";

contract EtfiAggregatorZap {
    using SafeERC20 for IERC20;
    using SafeERC20 for EtfiIndex;

    event ZapIn(
        address indexed _user,
        address indexed _index,
        address indexed _tokenZapped,
        uint256 _amountIn,
        uint256 _amountOut,
        uint256 _newEstimatedUsdPrice
    );

    event ZapOut(
        address indexed _user,
        address indexed _index,
        address indexed _tokenReceived,
        uint256 _amountIn,
        uint256 _amountOut,
        uint256 _newEstimatedUsdPrice
    );

    receive() external payable {}

    function getAmountsForZapIn(
        address _index,
        address _tokenToZap,
        uint256 _amountIn
    )
        external
        view
        returns (
            address _inputToken,
            address[] memory _tokens,
            uint256[] memory _amounts
        )
    {
        EtfiIndex _etfiIndex = EtfiIndex(_index);
        _tokens = _etfiIndex.getTokens();
        (uint256[] memory _currentWeights, ) = _etfiIndex
            .getUpdatedWeightsView();

        _amounts = new uint256[](_tokens.length);

        for (uint256 i = 0; i < _tokens.length; i++) {
            _amounts[i] = (_amountIn * _currentWeights[i]) / 10000;
        }

        _inputToken = _tokenToZap;
    }

    function zapIn(
        address _index,
        address _tokenToZap,
        uint256 _amountIn,
        address _aggregator,
        bytes[] calldata _swaps
    ) external {
        EtfiIndex _etfiIndex = EtfiIndex(_index);
        address[] memory _tokens = _etfiIndex.getTokens();

        IERC20(_tokenToZap).safeTransferFrom(
            msg.sender,
            address(this),
            _amountIn
        );
        IERC20(_tokenToZap).safeApprove(_aggregator, 0);
        IERC20(_tokenToZap).safeApprove(_aggregator, type(uint256).max);

        for (uint256 i = 0; i < _swaps.length; i++) {
            (bool _success, bytes memory result) = _aggregator.call{value: 0}(
                _swaps[i]
            );
            if (!_success) {
                assembly {
                    revert(add(result, 32), mload(result))
                }
            }
        }

        uint256[] memory _amounts = new uint256[](_tokens.length);

        for (uint256 i = 0; i < _tokens.length; i++) {
            _amounts[i] = IERC20(_tokens[i]).balanceOf(address(this));
            IERC20(_tokens[i]).safeApprove(_index, 0);
            IERC20(_tokens[i]).safeApprove(_index, _amounts[i]);
        }
        _etfiIndex.mint(_amounts);

        for (uint256 i = 0; i < _tokens.length; i++) {
            IERC20(_tokens[i]).safeTransfer(
                msg.sender,
                IERC20(_tokens[i]).balanceOf(address(this))
            );
        }

        uint256 _amountMinted = _etfiIndex.balanceOf(address(this));
        uint256 _indexPrice = _etfiIndex.getIndexPrice();

        _etfiIndex.safeTransfer(msg.sender, _amountMinted);

        for (uint256 i = 0; i < _tokens.length; i++) {
            uint256 _balance = IERC20(_tokens[i]).balanceOf(address(this));
            if (_balance > 0)
                IERC20(_tokens[i]).safeTransfer(msg.sender, _balance);
        }

        emit ZapIn(
            msg.sender,
            address(_etfiIndex),
            _tokenToZap,
            _amountIn,
            _amountMinted,
            _indexPrice
        );
    }

    function zapOut(
        address _index,
        address _tokenToReceive,
        uint256 _amountIn,
        address _aggregator,
        bytes[] calldata _swaps
    ) external {
        // Transfering token from the user
        IERC20(_index).safeTransferFrom(msg.sender, address(this), _amountIn);

        // Getting tokens
        address[] memory _tokens = EtfiIndex(_index).getTokens();

        // Redeeming the shares
        EtfiIndex(_index).redeem(_amountIn);

        for (uint256 i = 0; i < _tokens.length; i++) {
            IERC20(_tokens[i]).safeApprove(_aggregator, 0);
            IERC20(_tokens[i]).safeApprove(
                _aggregator,
                IERC20(_tokens[i]).balanceOf(address(this))
            );
        }

        // Swapping the tokens
        for (uint256 i = 0; i < _swaps.length; i++) {
            (bool _success, bytes memory result) = _aggregator.call{value: 0}(
                _swaps[i]
            );
            if (!_success) {
                assembly {
                    revert(add(result, 32), mload(result))
                }
            }
        }

        // Transfering the tokens to the user
        IERC20(_tokenToReceive).safeTransfer(
            msg.sender,
            IERC20(_tokenToReceive).balanceOf(address(this))
        );

        // Send the rest of the tokens to the user
        for (uint256 i = 0; i < _tokens.length; i++) {
            IERC20(_tokens[i]).safeTransfer(
                msg.sender,
                IERC20(_tokens[i]).balanceOf(address(this))
            );
        }

        // Emitting the event
        emit ZapOut(
            msg.sender,
            _index,
            _tokenToReceive,
            _amountIn,
            IERC20(_tokenToReceive).balanceOf(address(this)),
            EtfiIndex(_index).getIndexPrice()
        );
    }
}


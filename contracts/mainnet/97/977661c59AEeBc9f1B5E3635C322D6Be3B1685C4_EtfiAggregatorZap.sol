pragma solidity 0.8.9;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "prb-math/contracts/PRBMathSD59x18.sol";
import "./interfaces/IEtfiOmniOracle.sol";
import "hardhat/console.sol";

contract EtfiIndex is ERC20, Ownable, ReentrancyGuard {
    using SafeERC20 for IERC20;

    // Events
    event Mint(address indexed user, uint256 amount);
    event Burn(address indexed user, uint256 amount);
    event RebalanceStart(uint256 start, uint256 finish);
    event IndexSwap(
        address indexed user,
        address token0,
        address token1,
        uint256 amountIn,
        uint256 amountOut
    );

    // Variables
    address public treasury;
    address[] public tokens;
    uint256[] public tokensWeights;

    uint256[] public updatedWeights;
    uint256 public updatedWeightsTimestampLast;

    address public etfiOmniOracle;

    // Rebalancing window
    struct Swap {
        address tokenAvailable;
        address tokenRequested;
        uint256 amountRequested;
        uint256 price;
    }

    Swap[] public swaps;
    bool public isRebalancing = false;
    uint256 public lastRebalance;
    uint256 public rebalanceEvery;
    uint256 public rebalancingMaxWindow = 1 hours;

    // Fees
    uint256 public immutable PERCENT_DIVISOR = 10000;
    uint256 public depositFee = 20;
    uint256 public withdrawFee = 50;

    // 0.5% deviation from target weights tolerance
    uint256 public bpsTolerance = 50;

    // Constructor
    constructor(
        string memory _name,
        string memory _symbol,
        address _treasury,
        address[] memory _tokens,
        uint256[] memory _tokensWeights,
        address _etfiOmniOracle,
        uint256 _rebalanceEvery
    ) ERC20(_name, _symbol) {
        treasury = _treasury;
        tokens = _tokens;
        tokensWeights = _tokensWeights;

        updatedWeights = _tokensWeights;
        updatedWeightsTimestampLast = block.timestamp;

        etfiOmniOracle = _etfiOmniOracle;
        rebalanceEvery = _rebalanceEvery;
        lastRebalance = block.timestamp;
    }

    // Utilities, moving them to a library later
    modifier isInRebalancing() {
        require(isRebalancing == true, "not-rebalancing");

        if (block.timestamp > lastRebalance + rebalancingMaxWindow) {
            isRebalancing = false;
        } else {
            _;
        }
    }

    function abs(int256 x) private pure returns (uint256) {
        return uint256(x >= 0 ? x : -x);
    }

    function toggleRebalancing() external onlyOwner {
        isRebalancing = !isRebalancing;
    }

    function updateBpsTolerance(uint256 _bpsTolerance) external onlyOwner {
        require(_bpsTolerance <= 100, "tolerance-too-high");
        bpsTolerance = _bpsTolerance;
    }

    function tokenNormalizedBalance(
        address _token
    ) public view returns (uint256) {
        uint256 _balance = IERC20(_token).balanceOf(address(this));
        return _tokenNormalizeAmount(_token, _balance);
    }

    function _tokenNormalizeAmount(
        address _token,
        uint256 _amount
    ) internal view returns (uint256) {
        return (_amount * (10 ** (18 - IERC20Metadata(_token).decimals())));
    }

    function _calculateWeights(
        uint256[] memory _prices,
        uint256 _usdAUM
    ) internal view returns (uint256[] memory) {
        // initial minting must use initial weights
        if (_usdAUM == 0) {
            return tokensWeights;
        }

        uint256[] memory weights = new uint256[](_prices.length);

        for (uint256 i = 0; i < tokens.length; i++) {
            weights[i] =
                ((tokenNormalizedBalance(tokens[i]) *
                    _prices[i] *
                    PERCENT_DIVISOR) / _usdAUM) /
                1e18;
        }

        return weights;
    }

    function _getAUMUSD(
        uint256[] memory _prices
    ) internal view returns (uint256) {
        uint256 _aumUSD = 0;
        for (uint256 i = 0; i < tokens.length; i++) {
            _aumUSD += (tokenNormalizedBalance(tokens[i]) * _prices[i]) / 1e18;
        }

        return _aumUSD;
    }

    function getSwaps() external view returns (Swap[] memory) {
        return swaps;
    }

    function getTokens() external view returns (address[] memory) {
        return tokens;
    }

    function getRebalanceStatus() external view returns (bool) {
        return isRebalancing;
    }

    // Public functions
    function getPrices() public view returns (uint256[] memory) {
        uint256[] memory prices = new uint256[](tokens.length);

        for (uint8 i = 0; i < tokens.length; i++) {
            prices[i] =
                IEtfiOmniOracle(etfiOmniOracle).getLastPrice(
                    tokens[i],
                    (10 ** IERC20Metadata(tokens[i]).decimals())
                ) *
                1e12;
        }

        return prices;
    }

    function getUpdatedPrices() public returns (uint256[] memory) {
        uint256[] memory prices = new uint256[](tokens.length);

        for (uint8 i = 0; i < tokens.length; i++) {
            prices[i] =
                IEtfiOmniOracle(etfiOmniOracle).getUpdatedPrice(
                    tokens[i],
                    (10 ** IERC20Metadata(tokens[i]).decimals())
                ) *
                1e12;
        }

        return prices;
    }

    function getIndexPrice() public returns (uint256) {
        uint256[] memory _prices = getUpdatedPrices();
        uint256 _usdAUM = _getAUMUSD(_prices);
        uint256 _totalSupply = totalSupply();
        uint256 _indexPrice = (_totalSupply > 0)
            ? (_usdAUM / totalSupply())
            : 0;
        return _indexPrice;
    }

    function getWeights() external view returns (uint256[] memory) {
        uint256[] memory _prices = getPrices();
        uint256 _usdAUM = _getAUMUSD(_prices);
        uint256[] memory _weights = _calculateWeights(_prices, _usdAUM);
        return _weights;
    }

    function getUpdatedWeights() external returns (uint256[] memory) {
        uint256[] memory _prices = getUpdatedPrices();
        uint256 _usdAUM = _getAUMUSD(_prices);
        uint256[] memory _weights = _calculateWeights(_prices, _usdAUM);
        updatedWeights = _weights;
        updatedWeightsTimestampLast = block.timestamp;

        return _weights;
    }

    function getUpdatedWeightsView()
        external
        view
        returns (uint256[] memory, uint256)
    {
        return (updatedWeights, updatedWeightsTimestampLast);
    }

    function getTokensWeights() external view returns (uint256[] memory) {
        return tokensWeights;
    }

    function _getTokenPrices() internal returns (uint256[] memory) {
        uint256[] memory _prices = new uint256[](tokens.length);
        for (uint8 i = 0; i < tokens.length; i++) {
            _prices[i] =
                IEtfiOmniOracle(etfiOmniOracle).getUpdatedPrice(
                    tokens[i],
                    (10 ** IERC20Metadata(tokens[i]).decimals())
                ) *
                1e12;
        }
        return _prices;
    }

    function mint(uint256[] memory _assetsAmounts) external nonReentrant {
        require(_assetsAmounts.length == tokens.length, "wrong-amounts-len");

        uint256[] memory _weightsToUse = new uint256[](tokens.length);
        uint256[] memory _normalizedAmounts = new uint256[](tokens.length);
        uint256[] memory _desiredAmounts = new uint256[](tokens.length);
        uint256[] memory _prices = _getTokenPrices();

        uint256 _usdAUM = _getAUMUSD(_prices);
        uint256 _amountsWorth;

        if (isRebalancing == true) {
            _weightsToUse = tokensWeights;
        } else {
            _weightsToUse = _calculateWeights(_prices, _usdAUM);
        }

        for (uint8 i = 0; i < tokens.length; i++) {
            require(_assetsAmounts[i] > 0, "zero-amount");
            _normalizedAmounts[i] = _tokenNormalizeAmount(
                tokens[i],
                _assetsAmounts[i]
            );
            _amountsWorth += (_normalizedAmounts[i] * _prices[i]) / 1e18;
        }

        for (uint8 i = 0; i < tokens.length; i++) {
            uint256 _weightSupplied = (((_normalizedAmounts[i] * _prices[i]) /
                _amountsWorth) * PERCENT_DIVISOR) / 1e18;
            /*
            console.log(
                "weight supplied %s:",
                IERC20Metadata(tokens[i]).symbol(),
                _weightSupplied
            );
            console.log(
                "weight wanted %s:",
                IERC20Metadata(tokens[i]).symbol(),
                _weightsToUse[i]
            );
            console.log("---");

            */
            _desiredAmounts[i] = _assetsAmounts[i];

            require(
                _weightSupplied > _weightsToUse[i] - bpsTolerance,
                "too-low-weight"
            );

            if (_weightSupplied > _weightsToUse[i]) {
                _desiredAmounts[i] =
                    (_assetsAmounts[i] * _weightsToUse[i]) /
                    _weightSupplied;
            }

            IERC20(tokens[i]).safeTransferFrom(
                msg.sender,
                address(this),
                _desiredAmounts[i]
            );
        }

        _amountsWorth = 0;

        for (uint8 i = 0; i < tokens.length; i++) {
            _normalizedAmounts[i] = _tokenNormalizeAmount(
                tokens[i],
                _desiredAmounts[i]
            );
            _amountsWorth += (_normalizedAmounts[i] * _prices[i]) / 1e18;
        }

        if (_usdAUM == 0) {
            uint256 _sharesToUser = (_amountsWorth *
                (PERCENT_DIVISOR - depositFee)) / PERCENT_DIVISOR;
            uint256 _sharesToTreasury = _amountsWorth - _sharesToUser;

            _mint(msg.sender, _sharesToUser);
            _mint(treasury, _sharesToTreasury);
        } else {
            uint256 _amountToMint = (_amountsWorth * totalSupply()) / _usdAUM;

            uint256 _sharesToUser = (_amountToMint *
                (PERCENT_DIVISOR - depositFee)) / PERCENT_DIVISOR;
            uint256 _sharesToTreasury = _amountsWorth - _sharesToUser;

            _mint(msg.sender, _sharesToUser);
            _mint(treasury, _sharesToTreasury);
        }
    }

    function redeem(uint256 _amount) public nonReentrant {
        require(_amount > 0, "zero-amount");

        uint256 _usdAUM;
        uint256[] memory _prices = new uint256[](tokens.length);

        // building prices and finding how much the user need to withdraw
        for (uint8 i = 0; i < tokens.length; i++) {
            // normalizziamo l'amount
            _prices[i] =
                IEtfiOmniOracle(etfiOmniOracle).getUpdatedPrice(
                    tokens[i],
                    (10 ** IERC20Metadata(tokens[i]).decimals())
                ) *
                1e12;
            _usdAUM += (tokenNormalizedBalance(tokens[i]) * _prices[i]) / 1e18;
        }

        // calculating weights
        uint256[] memory _weights = _calculateWeights(_prices, _usdAUM);
        uint256 _userUSD = (_amount * _usdAUM) / totalSupply();

        // transfering shares from the user and burning them
        _burn(msg.sender, _amount);

        // returning tokens to the user and applying fees
        for (uint8 i = 0; i < tokens.length; i++) {
            // CHECK: some weights are like 5000 / 2500 / 2499
            uint256 _amountToTransfer = ((_userUSD * _weights[i] * 1e18) /
                PERCENT_DIVISOR /
                _prices[i]) /
                (10 ** (18 - IERC20Metadata(tokens[i]).decimals()));
            uint256 _maxBalance = IERC20(tokens[i]).balanceOf(address(this));
            if (_amountToTransfer > _maxBalance) {
                _amountToTransfer = _maxBalance;
            }
            uint256 _withdrawFeeAmount = (_amountToTransfer * withdrawFee) /
                PERCENT_DIVISOR;

            // console.log("amount to transfer: ", _amountToTransfer);
            // console.log("_withdrawFeeAmount: ", _withdrawFeeAmount);
            // console.log("token: ", tokens[i]);
            // console.log("price: ", _prices[i]);
            // console.log("weight: ", _weights[i]);

            IERC20(tokens[i]).safeTransfer(treasury, _withdrawFeeAmount);
            IERC20(tokens[i]).safeTransfer(
                msg.sender,
                _amountToTransfer - _withdrawFeeAmount
            );
        }
    }

    function quoteRedeem(
        uint256 _amount
    )
        external
        view
        returns (address[] memory _tokens, uint256[] memory _amounts)
    {
        require(_amount > 0, "zero-amount");

        _tokens = new address[](tokens.length);
        _tokens = tokens;

        _amounts = new uint256[](tokens.length);

        uint256 _usdAUM;
        uint256[] memory _prices = new uint256[](tokens.length);

        // building prices and finding how much the user need to withdraw
        for (uint8 i = 0; i < tokens.length; i++) {
            // normalizziamo l'amount
            _prices[i] =
                IEtfiOmniOracle(etfiOmniOracle).getLastPrice(
                    tokens[i],
                    (10 ** IERC20Metadata(tokens[i]).decimals())
                ) *
                1e12;
            _usdAUM += (tokenNormalizedBalance(tokens[i]) * _prices[i]) / 1e18;
        }

        // calculating weights
        uint256[] memory _weights = _calculateWeights(_prices, _usdAUM);
        uint256 _userUSD = (_amount * _usdAUM) / totalSupply();

        for (uint8 i = 0; i < tokens.length; i++) {
            uint256 _amountToTransfer = ((_userUSD * _weights[i] * 1e18) /
                PERCENT_DIVISOR /
                _prices[i]) /
                (10 ** (18 - IERC20Metadata(tokens[i]).decimals()));

            uint256 _maxBalance = IERC20(tokens[i]).balanceOf(address(this));
            if (_amountToTransfer > _maxBalance) {
                _amountToTransfer = _maxBalance;
            }
            uint256 _withdrawFeeAmount = (_amountToTransfer * withdrawFee) /
                PERCENT_DIVISOR;
            _amounts[i] = _amountToTransfer - _withdrawFeeAmount;
        }
    }

    function createSwaps() external isInRebalancing nonReentrant {
        _createSwaps();
    }

    function _createSwaps() internal {
        delete swaps;

        // this will also update oracles, no need to call updatedWeights
        uint256[] memory prices = getUpdatedPrices();
        uint256[] memory weightsToReach = tokensWeights;

        // AUM in usd
        uint256 usdAUM = _getAUMUSD(prices);

        // calculating weights
        uint256[] memory currentWeights = _calculateWeights(prices, usdAUM);

        // generating swaps
        int256[] memory operations = new int256[](tokens.length);

        for (uint8 i = 0; i < tokens.length; i++) {
            uint256 ideal_usd_amount = (usdAUM * weightsToReach[i]) /
                PERCENT_DIVISOR;
            operations[i] =
                ((int256(tokenNormalizedBalance(tokens[i]) * prices[i]) /
                    1e18) - int256(ideal_usd_amount)) *
                -1;
        }

        for (uint8 x = 0; x < operations.length; x++) {
            for (uint8 y = 0; y < operations.length; y++) {
                if (
                    x != y &&
                    weightsToReach[x] != currentWeights[x] &&
                    weightsToReach[y] != currentWeights[y]
                ) {
                    if (operations[x] > 0 && operations[y] < 0) {
                        if (abs(operations[x]) >= abs(operations[y])) {
                            uint256 _amount_of_token = abs(
                                operations[y] * 1e18
                            ) / prices[y];
                            uint256 _amount_to_receive = abs(
                                operations[y] * 1e18
                            ) / prices[x];

                            swaps.push(
                                Swap(
                                    tokens[y],
                                    tokens[x],
                                    _amount_to_receive,
                                    (_amount_to_receive * 1e18) /
                                        _amount_of_token
                                )
                            );

                            operations[x] =
                                operations[x] -
                                int256(abs(operations[y]));
                            operations[y] = 0;
                        } else {
                            uint256 _amount_of_token = abs(
                                operations[x] * 1e18
                            ) / prices[y];
                            uint256 _amount_to_receive = abs(
                                operations[x] * 1e18
                            ) / prices[x];
                            /*
                            console.log("receiveToken:", IERC20Metadata(tokens[x]).symbol());
                            console.log("receivePrice:", prices[x]);
                            console.log("_amount_to_receive", _amount_to_receive);
                            console.log("sendToken:", IERC20Metadata(tokens[y]).symbol());
                            console.log("_amount_of_token", _amount_of_token);
                            */
                            swaps.push(
                                Swap(
                                    tokens[y],
                                    tokens[x],
                                    _amount_to_receive,
                                    (_amount_to_receive * 1e18) /
                                        _amount_of_token
                                )
                            );

                            operations[y] =
                                operations[y] +
                                int256(abs(operations[x]));
                            operations[x] = 0;
                        }
                    }
                }
            }
        }
    }

    function startRebalance() external {
        // check if we can rebalance
        require(isRebalancing == false, "rebalance-in-progress");
        require(
            block.timestamp > lastRebalance + rebalanceEvery,
            "rebalance-interval-not-reached"
        );

        // reset swaps
        delete swaps;

        // set rebalance start
        lastRebalance = block.timestamp;
        isRebalancing = true;

        // TODO: get weights from external contract
        // hardcoded will be provided by external contracts
        uint256[] memory weightsToReach = tokensWeights;

        require(weightsToReach.length == tokens.length, "invalid-weights");
        uint256 totalWeights;

        for (uint8 i = 0; i < weightsToReach.length; i++) {
            totalWeights += weightsToReach[i];
            tokensWeights[i] = weightsToReach[i];
        }

        require(totalWeights == PERCENT_DIVISOR, "invalid-weights");
        // create new one
        _createSwaps();

        // emit
        emit RebalanceStart(
            block.timestamp,
            block.timestamp + rebalancingMaxWindow
        );
    }

    // TODO: add also check on window
    function swap(
        address tokenRequested,
        address tokenAvailable,
        uint256 amount
    ) external nonReentrant {
        require(isRebalancing == true, "rebalance-not-in-progress");
        require(
            block.timestamp < lastRebalance + rebalancingMaxWindow,
            "rebalance-window-ended"
        );

        // check if the swap is in the list
        bool found = false;
        uint256 swapIndex = 0;
        for (uint8 i = 0; i < swaps.length; i++) {
            if (
                swaps[i].tokenAvailable == tokenAvailable &&
                swaps[i].tokenRequested == tokenRequested
            ) {
                found = true;
                swapIndex = i;
                break;
            }
        }

        // check if is valid swap
        require(found == true, "swap-not-found");

        // check amount doesn't overflow swap request
        require(amount <= swaps[swapIndex].amountRequested, "amount-too-high");

        // transfer tokens from the user
        IERC20(tokenRequested).safeTransferFrom(
            msg.sender,
            address(this),
            amount
        );

        // calculating amount to send to the user
        // uint256 amountToTransfer = ((amount * 1e18) / swaps[swapIndex].price);
        uint256 amountToTransfer = (amount * 1e18) / swaps[swapIndex].price;

        //console.log("amountToTransfer", amountToTransfer);
        if (
            amountToTransfer > IERC20(tokenAvailable).balanceOf(address(this))
        ) {
            amountToTransfer = IERC20(tokenAvailable).balanceOf(address(this));
        }

        // transfer tokens to the user
        IERC20(tokenAvailable).safeTransfer(msg.sender, amountToTransfer);

        // updating the storage
        if (amount == swaps[swapIndex].amountRequested) {
            delete swaps[swapIndex];
        } else {
            swaps[swapIndex].amountRequested =
                swaps[swapIndex].amountRequested -
                amount;
        }

        // check if rebalance is over
        bool isOver = true;
        for (uint256 x = 0; x < swaps.length; x++) {
            if (swaps[x].amountRequested > 0) {
                isOver = false;
                break;
            }
        }

        if (isOver == true) {
            isRebalancing = false;
            delete swaps;
        }

        // emit
        emit IndexSwap(
            msg.sender,
            tokenRequested,
            tokenAvailable,
            amount,
            amountToTransfer
        );
    }
}


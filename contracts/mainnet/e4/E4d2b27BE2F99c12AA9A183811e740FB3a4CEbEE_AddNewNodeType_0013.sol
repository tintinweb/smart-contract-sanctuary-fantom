pragma solidity ^0.8.9;

import {INodeRewards} from "./INodeRewards.sol";
import {INodes} from "../nodes/interfaces/INodes.sol";
import {LibNodes} from "../nodes/libraries/LibNodes.sol";
import {LibNodeFee} from "../node-fees/LibNodeFee.sol";
import {LibNft} from "../nft/libraries/LibNft.sol";
import {LibNodeTypes} from "../nodes/libraries/LibNodeTypes.sol";
import {LibSoloToken} from "../solo-token/LibSoloToken.sol";
import {LibTokenDistributor} from "../token-distributor/LibTokenDistributor.sol";
import {SafeMath} from "@openzeppelin/contracts/utils/math/SafeMath.sol";

library LibNodeRewards {
    using SafeMath for uint256;

    struct NodeRewards {
        uint256 lastClaimedAt;
        uint256 totalClaimed;
    }

    struct Storage {
        mapping(uint256 => NodeRewards) nodeRewardsMap;
        mapping(uint256 => uint256) nodeTypeRewardsPerSecond;
        mapping(uint256 => uint256) nodeTypeTotalClaimedMap;
        mapping(address => uint256) accountTotalClaimedMap;
        uint256 claimTax;
        address rewardsVaultAddress;
    }

    bytes32 internal constant STORAGE_SLOT =
        keccak256("solarprotocol.contracts.node-rewards.LibNodeRewards");

    /**
     * @dev Returns the storage.
     */
    function _storage() private pure returns (Storage storage s) {
        bytes32 slot = STORAGE_SLOT;
        // solhint-disable-next-line no-inline-assembly
        assembly {
            s.slot := slot
        }
    }

    /**
     * @dev Sets the `rewardsPerSecond` for `nodeTypeId`.
     */
    function setNodeTypeReward(uint256 nodeTypeId, uint256 rewardsPerSecond)
        internal
    {
        LibNodeTypes.enforceNodeTypeExists(nodeTypeId);

        _storage().nodeTypeRewardsPerSecond[nodeTypeId] = rewardsPerSecond;
    }

    /**
     * @dev Sets the `rewardsVaultAddress`.
     */
    function setRewardsVaultAddress(address rewardsVaultAddress) internal {
        _storage().rewardsVaultAddress = rewardsVaultAddress;
    }

    /**
     * @dev Sets the `claimTax`.
     */
    function setClaimTax(uint256 claimTax) internal {
        _storage().claimTax = claimTax;
    }

    /**
     * @dev Returns the `rewardsVaultAddress`.
     */
    function getRewardsVaultAddress() internal view returns (address) {
        return _storage().rewardsVaultAddress;
    }

    /**
     * @dev Returns the `claimTax`.
     */
    function getClaimTax() internal view returns (uint256) {
        return _storage().claimTax;
    }

    /**
     * @dev Claims the pending rewards of a node by sending it to the node owner
     * and resetting lastClaimedAt.
     */
    function claim(uint256 nodeId) internal returns (uint256 pendingRewards) {
        uint256 typeId = LibNodes.getNode(nodeId).typeId;
        pendingRewards = claim(nodeId, typeId);
    }

    /**
     * @dev Claims the pending rewards of a node by sending it to the node owner
     * and resetting lastClaimedAt.
     */
    function claim(uint256 nodeId, uint256 typeId)
        internal
        returns (uint256 pendingRewards)
    {
        address owner = LibNft.ownerOf(nodeId);
        pendingRewards = calculatePendingRewards(nodeId, typeId);

        if (pendingRewards > 0) {
            // solhint-disable-next-line check-send-result
            LibSoloToken.send(
                _storage().rewardsVaultAddress,
                owner,
                pendingRewards,
                "",
                "",
                false
            );

            // solhint-disable-next-line not-rely-on-time
            _storage().nodeRewardsMap[nodeId].lastClaimedAt = block.timestamp;
            _storage().nodeRewardsMap[nodeId].totalClaimed += pendingRewards;
            _storage().nodeTypeTotalClaimedMap[typeId];
            _storage().accountTotalClaimedMap[owner] += pendingRewards;
        }
    }

    function claimAmount(address account, uint256 minAmount)
        internal
        returns (uint256 claimedRewards)
    {
        INodes.Node[] memory nodes = LibNodes.getNodesOwnedBy(account);

        for (uint256 index = 0; index < nodes.length; ++index) {
            if (LibNodeFee.isNodeExpired(nodes[index].tokenId)) {
                continue;
            }

            claimedRewards += LibNodeRewards.claim(
                nodes[index].tokenId,
                nodes[index].typeId
            );

            if (claimedRewards >= minAmount) {
                break;
            }
        }

        if (claimedRewards < minAmount) {
            revert INodeRewards.NotEnoughRewards();
        }
    }

    /**
     * @dev Calculates the pending rewards for a list of `nodeIds`.
     */
    function calculatePendingRewards(uint256[] memory nodeIds)
        internal
        view
        returns (uint256 rewards)
    {
        for (uint256 index = 0; index < nodeIds.length; ++index) {
            rewards += calculatePendingRewards(nodeIds[index]);
        }
    }

    /**
     * @dev Calculates the pending rewards for `nodeId`.
     */
    function calculatePendingRewards(uint256 nodeId)
        internal
        view
        returns (uint256 rewards)
    {
        rewards = calculatePendingRewards(
            nodeId,
            LibNodes.getNode(nodeId).typeId
        );
    }

    /**
     * @dev Calculates the pending rewards for `nodeId` of type `typeId`.
     */
    function calculatePendingRewards(uint256 nodeId, uint256 typeId)
        internal
        view
        returns (uint256 rewards)
    {
        uint256 nodeTypeRewardsPerSecond = getNodeTypeRewardsPerSecond(typeId);

        if (nodeTypeRewardsPerSecond == 0) {
            return 0;
        }

        return
            // solhint-disable-next-line not-rely-on-time
            (block.timestamp - getLastClaimedAt(nodeId)) *
            nodeTypeRewardsPerSecond;
    }

    /**
     * @dev Returns the rewardsPerSecond for `nodeTypeId`.
     */
    function getNodeTypeRewardsPerSecond(uint256 nodeTypeId)
        internal
        view
        returns (uint256 rewards)
    {
        rewards = _storage().nodeTypeRewardsPerSecond[nodeTypeId];
    }

    /**
     * @dev Returns `lastClaimedAt` for `nodeId` and falls back to `createdAt`.
     */
    function getLastClaimedAt(uint256 nodeId)
        internal
        view
        returns (uint256 lastClaimedAt)
    {
        lastClaimedAt = _storage().nodeRewardsMap[nodeId].lastClaimedAt;

        if (lastClaimedAt == 0) {
            lastClaimedAt = LibNodes.getNode(nodeId).createdAt;
        }
    }

    function getTotalRewardsClaimed(uint256 typeId)
        internal
        view
        returns (uint256)
    {
        return _storage().nodeTypeTotalClaimedMap[typeId];
    }

    function getTotalRewardsClaimed(address account)
        internal
        view
        returns (uint256)
    {
        return _storage().accountTotalClaimedMap[account];
    }

    function getNodeRewardsInfo(uint256 nodeId)
        internal
        view
        returns (INodeRewards.NodeRewardsInfoResponse memory response)
    {
        LibNodeRewards.NodeRewards memory nodeRewards = _storage()
            .nodeRewardsMap[nodeId];

        response.lastClaimedAt = nodeRewards.lastClaimedAt;
        response.totalClaimed = nodeRewards.totalClaimed;
        response.pendingRewards = calculatePendingRewards(nodeId);
    }

    function withdrawTax(address account, uint256 amount) internal {
        uint256 claimTax = _storage().claimTax;

        if (claimTax == 0) {
            return;
        }

        LibTokenDistributor.transfer(
            keccak256("CLAIM_TAX"),
            account,
            amount.mul(claimTax).div(100)
        );
    }
}


        // Total fees paid
        uint256 totalPaid;
        // Timestamp of when the fee was last paid
        uint256 lastPaidAt;
        // Timestamp until next fee payment
        uint256 expiresAt;
    }

    struct NodeTypeFeesResponse {
        uint256 nodeTypeId;
        uint256 feeAmount;
    }

    /**
     * @dev Emitted when a NodeFee is Paid.
     */
    event NodeFeePaid(uint256 indexed nodeId, address indexed account);

    /**
     * @dev Emitted when a NodeFee is Paid.
     */
    event NodeFeePaidByType(uint256 indexed typeId, address indexed account);

    error FeeTimeLimit(uint256 numberDays);
    error FeeStepLimit();
    error NodeExpireTimeLimit();
    error NodeIsExpired(uint256 nodeId);

    /**
     * @dev Pay NodeFee for Node
     * @param nodeId Id of the node.
     * @param numberDays number of days to pay for maintenance.
     */
    function payFee(uint256 nodeId, uint256 numberDays) external;

    /**
     * @dev Pay NodeFee for nodes with the same NodeType
     * @param nodeIds List of node ids to pay the fees for.
     * @param numberDays number of days to pay for maintenance.
     */
    function payFees(uint256[] memory nodeIds, uint256 numberDays) external;

    /**
     * @dev Get a NodeFee Object
     * @param nodeId Node id
     */
    function getNodeFee(uint256 nodeId)
        external
        view
        returns (NodeFee memory nodeFee);

    /**
     * @dev Get the node fee of nodeType
     * @param nodeTypeId NodeType id
     */
    function getNodeTypeFee(uint256 nodeTypeId)
        external
        view
        returns (uint256 feeAmount);
}


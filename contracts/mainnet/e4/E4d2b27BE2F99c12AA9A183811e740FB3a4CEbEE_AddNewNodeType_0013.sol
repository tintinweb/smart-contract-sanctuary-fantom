import {INodeFee} from "./INodeFee.sol";
import {INodes} from "../nodes/interfaces/INodes.sol";
import {LibNodes} from "../nodes/libraries/LibNodes.sol";
import {LibTokenDistributor} from "../token-distributor/LibTokenDistributor.sol";
import {SafeMath} from "@openzeppelin/contracts/utils/math/SafeMath.sol";

library LibNodeFee {
    using SafeMath for uint256;

    struct Storage {
        mapping(uint256 => uint256) nodeTypeFee;
        mapping(uint256 => INodeFee.NodeFee) nodeFeeMap;
        uint256 minDays;
        uint256 maxDays;
        uint256 step;
    }

    /**
     *   @dev name the storage slot
     */
    bytes32 internal constant STORAGE_SLOT =
        keccak256("solarprotocol.contracts.node-fees.LibNodeFee");

    uint256 internal constant FEE_MULTIPLIER = 1e11;

    /**
     * @dev Returns the storage.
     */
    function _storage() private pure returns (Storage storage s) {
        bytes32 slot = STORAGE_SLOT;
        // solhint-disable-next-line no-inline-assembly
        assembly {
            s.slot := slot
        }
    }

    /**
     * @dev Enforce that the months in advance being paid for are between minDays and maxDays
     * @param numberDays number of days to check
     */
    function enforceTimeLimits(uint256 numberDays) internal view {
        Storage storage s = _storage();

        if (numberDays < s.minDays || numberDays > s.maxDays) {
            revert INodeFee.FeeTimeLimit(numberDays);
        }

        if (s.step > 0 && numberDays % s.step != 0) {
            revert INodeFee.FeeStepLimit();
        }
    }

    /**
     * @dev Enforce the node expiration timestamp does not exceed the maximum possible timeframe.
     */
    function enforceNodeTimeLimit(uint256 expiresAt) internal view {
        // solhint-disable-next-line not-rely-on-time
        if (expiresAt > block.timestamp.add(_storage().maxDays.mul(1 days))) {
            revert INodeFee.NodeExpireTimeLimit();
        }
    }

    /**
     * @dev Reverts if the node with `nodeId` is expired.
     * @param nodeId Node id.
     */
    function enforceNodeIsNotExpired(uint256 nodeId) internal view {
        if (isNodeExpired(nodeId)) {
            revert INodeFee.NodeIsExpired(nodeId);
        }
    }

    /**
     * @dev Set the Monthly Fees of Node Type
     * @param nodeTypeId NodeType id
     * @param feeAmount Fee amount of NodeType
     *
     */
    function setNodeTypeFee(uint256 nodeTypeId, uint256 feeAmount) internal {
        _storage().nodeTypeFee[nodeTypeId] = feeAmount;
    }

    /**
     * @dev Get a NodeFee Object
     * @param nodeTypeId NodeType id
     */
    function getNodeTypeFee(uint256 nodeTypeId)
        internal
        view
        returns (uint256 feeAmount)
    {
        feeAmount = _storage().nodeTypeFee[nodeTypeId];
    }

    /**
     * @dev Pay NodeFee for Node
     * @param nodeId Id of the node.
     * @param numberDays Number of days to pay the fees.
     */
    function payFee(uint256 nodeId, uint256 numberDays)
        internal
        returns (uint256 feeAmount)
    {
        return payFee(LibNodes.getNode(nodeId), numberDays);
    }

    /**
     * @dev Pay NodeFee for Node
     * @param node Node object.
     * @param numberDays Number of days to pay the fees.
     */
    function payFee(INodes.Node memory node, uint256 numberDays)
        internal
        returns (uint256 feeAmount)
    {
        feeAmount = calculateFeeAmount(node.typeId, numberDays);
        INodeFee.NodeFee memory nodeFee = getNodeFee(node.tokenId);

        // solhint-disable-next-line not-rely-on-time
        nodeFee.lastPaidAt = block.timestamp;
        nodeFee.totalPaid += feeAmount;
        nodeFee.expiresAt = calculateNewExpitesAt(
            nodeFee.expiresAt,
            numberDays
        );

        enforceNodeTimeLimit(nodeFee.expiresAt);

        _storage().nodeFeeMap[node.tokenId] = nodeFee;
    }

    /**
     * @dev Distribute fee
     * @param account address of account
     * @param amount amount to transfer
     */
    function distributeFee(address account, uint256 amount) internal {
        LibTokenDistributor.transfer(
            keccak256("PAY_NODE_FEES"),
            account,
            amount
        );
    }

    /**
     * @dev Get a NodeFee Object
     * @param nodeId Node id
     */
    function getNodeFee(uint256 nodeId)
        internal
        view
        returns (INodeFee.NodeFee memory nodeFee)
    {
        nodeFee = _storage().nodeFeeMap[nodeId];

        if (nodeFee.expiresAt == 0) {
            nodeFee.expiresAt = LibNodes.getNode(nodeId).createdAt + 30 days;
        }
    }

    /**
     * @dev Returns true if the node with `nodeId`is expired;
     * @param nodeId Node id
     */
    function isNodeExpired(uint256 nodeId) internal view returns (bool) {
        // solhint-disable-next-line not-rely-on-time
        return block.timestamp > getNodeFee(nodeId).expiresAt;
    }

    /**
     * @dev Calculate the Fee amount
     * @param nodeTypeId id of NodeType
     * @param numberDays months represented in seconds
     */
    function calculateFeeAmount(uint256 nodeTypeId, uint256 numberDays)
        internal
        view
        returns (uint256 feeAmount)
    {
        feeAmount = _storage().nodeTypeFee[nodeTypeId].mul(numberDays).div(
            FEE_MULTIPLIER
        );
    }

    function calculateNewExpitesAt(uint256 oldExpiresAt, uint256 numberDays)
        internal
        pure
        returns (uint256 newExpiresAt)
    {
        newExpiresAt = oldExpiresAt.add(numberDays.mul(1 days));
    }

    function setMinDays(uint256 minDays) internal {
        _storage().minDays = minDays;
    }

    function setMaxDays(uint256 maxDays) internal {
        _storage().maxDays = maxDays;
    }

    function setStep(uint256 step) internal {
        _storage().step = step;
    }
}


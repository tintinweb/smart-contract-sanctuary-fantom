pragma solidity ^0.8.9;

import {INodes} from "../interfaces/INodes.sol";
import {LibNodeTypes} from "./LibNodeTypes.sol";
import {LibNft} from "../../nft/libraries/LibNft.sol";
import {IRedeem} from "@solarprotocol/presale/contracts/redeem/IRedeem.sol";

library LibNodes {
    struct Storage {
        mapping(uint256 => INodes.Node) nodeMap;
        mapping(uint256 => uint256[]) nodeTypeNodes;
        uint256 totalNodes;
        IRedeem presaleContract;
    }

    bytes32 internal constant STORAGE_SLOT =
        keccak256("solarprotocol.contracts.nodes.LibNodes");

    /**
     * @dev Returns the storage.
     */
    function _storage() private pure returns (Storage storage s) {
        bytes32 slot = STORAGE_SLOT;
        // solhint-disable-next-line no-inline-assembly
        assembly {
            s.slot := slot
        }
    }

    /**
     * @dev Emitted when a new node was created.
     */
    event NodeCreated(uint256 indexed tokenId, address account);

    /**
     * @dev Creates a node of `typeId`.
     *
     * @param typeId Id of the node type.
     * @return tokenId Id of the ERC1155 token.
     */
    function createNode(address account, uint256 typeId, bool isPresale) internal returns (uint256 tokenId) {
        tokenId = LibNft.mintNft(account);

        INodes.Node memory node = INodes.Node({
            tokenId: tokenId,
            typeId: LibNodeTypes.getNodeType(typeId).id,
            // solhint-disable-next-line not-rely-on-time
            createdAt: block.timestamp,
            isPresale: isPresale
        });
        


        _storage().nodeMap[tokenId] = node;
        _storage().nodeTypeNodes[typeId].push(tokenId);
        _storage().totalNodes++;

        emit NodeCreated(tokenId, account);
    }

    /**
     * @dev Sets the solar presale contract
     */
    function setPresaleContract(IRedeem presaleContract) internal {
        _storage().presaleContract = presaleContract;
    }

    /**
     * @dev Returns the presale contract.
     */
    function getPresaleContract() internal view returns (IRedeem) {
        return _storage().presaleContract;
    }

    /**
     * @dev Returns Node information of `tokenId`.
     *
     * @param tokenId Id of the token associated with the node.
     * @return node Node struct.
     */
    function getNode(uint256 tokenId) internal view returns (INodes.Node memory node) {
        node = _storage().nodeMap[tokenId];

        if (node.tokenId < 1 || node.tokenId != tokenId) {
            revert INodes.NodeNotFound(tokenId);
        }
    }

    /**
     * @dev Returns a list of nodes from `tokenIds`.
     *
     * @param tokenIds Array of the tokens associated with the nodes.
     * @return nodes Array of Node structs.
     */
    function getNodes(uint256[] memory tokenIds)
        internal
        view
        returns (INodes.Node[] memory) {
            INodes.Node[] memory nodes = new INodes.Node[](tokenIds.length);
            for(uint256 index = 0; index < tokenIds.length; ++index) {
                nodes[index] = getNode(tokenIds[index]);
            }

            return nodes;
        }

    /**
     * @dev Returns a list of all nodes owned by `account`.
     *
     * @param account address of node owner
     * @return nodeList array of Node structs
     */
    function getNodesOwnedBy(address account)
        internal
        view
        returns (INodes.Node[] memory)
    {
        return getNodes(LibNft.getNftsOf(account));
    }

    /**
     * @dev Returns a list of tokenIds holding a node of `typeId`.
     * 
     * @param typeId Id of the node type.
     * @return tokenIds Array with tokenIds of the nodes.
     */
    function getNodeIdsOfType(uint256 typeId) internal view returns(uint256[] memory) {
        return _storage().nodeTypeNodes[typeId];
    }

    /**
     * @dev Returns a total number of nodes.
     */
    function getTotalNodes() internal view returns (uint256) {
        return _storage().totalNodes;
    }

    /**
     * @dev Returns a total number of nodes of a certain type.
     *
     * @param typeId Id of the node type.
     */
    function getTotalNodesOfType(uint256 typeId)
        internal
        view
        returns (uint256)
    {
        return _storage().nodeTypeNodes[typeId].length;
    }
}


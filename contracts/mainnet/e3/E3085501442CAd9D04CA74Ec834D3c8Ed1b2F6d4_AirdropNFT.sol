contract AirdropNFT is Ownable {
    IERC721 public nft;

    constructor(IERC721 _nft) {
        nft = _nft;
    }

    function transferList(
        address[] memory _buyersOne,
        uint16[] calldata tokenIds
    ) public {
        for (uint256 i; i < _buyersOne.length; i++) {
            nft.transferFrom(msg.sender, _buyersOne[i], tokenIds[i]);
        }
    }

    function setNFT(IERC721 _nft) public onlyOwner {
            nft = _nft;
    }
}